# ==========================================
# LLM Data Lab Backend Configuration
# ==========================================
# Copy this file to .env and fill in your actual values
# NEVER commit .env to version control!

# ==========================================
# Authentication & Security (REQUIRED)
# ==========================================
# Generate with: openssl rand -hex 32
# Must be at least 32 characters
JWT_SECRET_KEY=your-super-secret-jwt-key-change-this-in-production-must-be-32-chars
JWT_ALGORITHM=HS256
ACCESS_TOKEN_EXPIRES_MINUTES=43200

# Optional: Separate key for encrypting stored provider credentials
# If not set, will use JWT_SECRET_KEY
CREDENTIALS_SECRET_KEY=

# ==========================================
# Database
# ==========================================
# SQLite (default)
DATABASE_URL=sqlite+aiosqlite:///./llm_data_lab.db

# PostgreSQL (production)
# DATABASE_URL=postgresql+asyncpg://user:password@localhost/llm_data_lab

# ==========================================
# OpenAI Configuration (Optional)
# ==========================================
OPENAI_API_KEY=
OPENAI_DEFAULT_MODELS=["gpt-4o","gpt-4o-mini","gpt-4-turbo"]

# ==========================================
# Anthropic Configuration (Optional)
# ==========================================
ANTHROPIC_API_KEY=

# ==========================================
# DeepSeek Configuration (Optional)
# ==========================================
DEEPSEEK_API_KEY=
DEEPSEEK_BASE_URL=https://api.deepseek.com
DEEPSEEK_DEFAULT_MODELS=["deepseek-chat","deepseek-coder"]

# ==========================================
# Qwen (DashScope) Configuration (Optional)
# ==========================================
DASHSCOPE_API_KEY=
DASHSCOPE_BASE_URL=https://dashscope.aliyuncs.com
QWEN_DEFAULT_MODELS=["qwen-turbo","qwen-plus","qwen-max"]

# ==========================================
# SiliconFlow Configuration (Optional)
# ==========================================
SILICONFLOW_API_KEY=
SILICONFLOW_BASE_URL=https://api.siliconflow.cn
SILICONFLOW_DEFAULT_MODELS=["Qwen/Qwen2.5-7B-Instruct","deepseek-ai/DeepSeek-V2.5"]

# ==========================================
# Code Execution Limits
# ==========================================
MAX_CODE_EXECUTION_SECONDS=60
MAX_CODE_EXECUTION_MEMORY_MB=768

# ==========================================
# File Storage (Optional - uses defaults if not set)
# ==========================================
# UPLOAD_DIR=./uploaded_datasets
# ARTIFACTS_DIR=./analysis_artifacts
# ALLOWED_UPLOAD_EXTENSIONS=["csv","xlsx","xls"]

# ==========================================
# Application (Optional)
# ==========================================
# APP_NAME=LLM Data Lab
# ENVIRONMENT=development
# FRONTEND_ORIGIN=http://localhost:3000
